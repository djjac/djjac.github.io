sub terbekesub

  vooraad.value = ""
   aantal_k.value = ""
   If IsNumeric(input.value) Then
   
pallet = input.value / 1440
if pallet  > 0 then
pallet = Int(pallet)
end if

if pallet > 0 then plus =" + " else plus =" " 

doos =  ( input.value - (pallet * 1440))/ 6
doos = Int (doos) 
bakken =  input.value - (pallet * 1440) - doos * 6

if pallet > 0 then 
pallet =" " &pallet & " pallet"
else 
pallet = ""
end if

if doos > 0 then 
if doos > 1 then 
hoog = doos/16
hoog = Int (hoog) 
resthoog = doos - (hoog*16)

hoog = hoog & " hoog"
if resthoog > 0 then
resthoog = " + " & resthoog & " dozen" 
end if

doos = plus & doos &  " dozen (" & hoog &  resthoog  &  ")"
else 
doos = plus & doos &  " doos " 
end if
else doos = ""
end if

if bakken > 0 then 
if bakken > 1 then 
bakken = " + " & bakken & " bakken  " 
else 
bakken = " + " & bakken & " bak  " 
end if
else bakken = ""
end if

output = pallet & doos & bakken 

inputdec= Round(input.value/6)

info.value =" aantal dozen: "& inputdec &vbNewline &vbNewline &  output

else
Msgbox foutmelding
end if
end sub
	
