Sub inkrat
  vooraad.value = ""
   aantal_k.value = ""
If IsNumeric(input.value)  Then

 pallet = input.value / 240
 if pallet  > 0 then
 pallet = Int(pallet)
 end if

if pallet > 0 then plus ="+ " else plus ="" 


kratst =  ( input.value - (pallet * 240))/ 6
krat = kratst

krat = Int (krat) 
bakken =  input.value - (pallet * 240 ) - krat * 6



if pallet > 0 then 
pallet = pallet & " pallet "
 plus ="+ "
else 
pallet = ""
 plus =" " 
end if



if krat > 0 then 
if krat > 1 then 
krat = plus & Round(krat,1) &  " kratten" 

else 
krat = plus & krat &  " krat " 
end if
else krat = ""
end if

if bakken > 0 then 
if bakken > 1 then 
bakken = " + " & bakken & " bakken  " 
else 
bakken = " + " & bakken & " bak  " 
end if
else bakken = ""
end if



if kratst > 0 then 
if kratst > 1 then 
hoog = kratst/4
hoog = Int (hoog) 
resthoog = kratst - (hoog*4)

hoog = hoog & " hoog"
if resthoog > 0 then
resthoog = " + " & int (resthoog) & " kratten" 
end if

kratst = plus & Int(kratst) &  " kratten (" & hoog &  resthoog  &  ")"
else 
kratst = plus & kratst &  " kraten " 
end if
else kratst = ""
end if

output = " 6 in krat:"&vbNewline &vbNewline&" " & pallet & kratst & bakken


info.value=output


else	
		
		Msgbox foutmelding


end if
end sub



